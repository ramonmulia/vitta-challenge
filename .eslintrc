{
  "extends": "airbnb",
  "rules": {
    "no-use-before-define": ["error", { "functions": false, "classes": false }],
    "comma-dangle": ["error", "never"],
    "no-underscore-dangle": ["error", { "allow": ["_id", "_embedded", "_links"] }],
    "camelcase": 2,
    "max-len": ["error", 200]
  },
  "env": {
    "node": true,
    "mocha": true
  }
}
